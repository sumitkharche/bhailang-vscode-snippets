{
    "name": "bhailang-vscode-snippets",
    "displayName": "Bhailang VSCode Snippets",
    "description": "VS Code snippets for Bhailang",
    "version": "1.0.1",
    "icon": "./images/bhailang-vs-icon-1.PNG",
    "publisher": "Sumit Kharche",
    "repository": {
        "type": "git",
        "url": "https://github.com/sumitkharche/bhailang-vscode-snippets"
    },
    "engines": {
        "vscode": "^1.65.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "bhailang",
            "aliases": [
                "Bhailang",
                "bhailang"
            ],
            "extensions": [
                ".bhai"
            ],
            "configuration": "./language-configuration.json"
        }],   
        "grammars": [
            {
                "language": "bhailang",
                "scopeName": "source.bhailang",
                "path": "./syntaxes/bhailang.tmLanguage.json"
            }
        ],    
        "snippets": [
            {
                "language": "bhailang",
                "path": "./snippets/bhailang-snippets.json"
            }
        ]
    }
}
